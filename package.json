{
  "name": "puppeteer",
  "version": "2.0.0-post",
  "description": "A high-level API to control headless Chrome over the DevTools Protocol",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": "github:puppeteer/puppeteer",
  "engines": {
    "node": ">=8.16.0"
  },
  "puppeteer": {
    "chromium_revision": "706915"
  },
  "scripts": {
    "prepare": "npm run build",
    "install": "node install.js",
    "unit": "node test/test.js",
    "fjunit": "PUPPETEER_PRODUCT=juggler node test/test.js",
    "funit": "PUPPETEER_PRODUCT=firefox node test/test.js",
    "debug-unit": "node --inspect-brk test/test.js",
    "prebuild": "rimraf ./lib",
    "build": "tsc",
    "build-protocol": "node -r ts-node/register ./scripts/generate-protocol-types",
    "pretest": "npm run build && npm run lint",
    "test": "npm run coverage && npm run test-types && npm run unit-test-runner && npm run test-doclint",
    "coverage": "cross-env COVERAGE=true npm run unit",
    "test-doclint": "node utils/doclint/check_public_api/test/test.js && node utils/doclint/preprocessor/test.js",
    "unit-test-runner": "node utils/testrunner/test/test.js",
    "lint": "eslint . --ext .js,.ts -f codeframe",
    "doc": "node utils/doclint/cli.js",
    "apply-next-version": "node utils/apply_next_version.js",
    "bundle": "npx browserify -r ./index.js:puppeteer -o utils/browser/puppeteer-web.js",
    "test-types": "tsc -p test/types/tsconfig.json && tsc -p test/types/tsconfig.interop.json",
    "unit-bundle": "node utils/browser/test.js"
  },
  "author": "The Chromium Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "debug": "^4.1.1",
    "extract-zip": "^1.6.7",
    "https-proxy-agent": "^4.0.0",
    "mime": "^2.4.4",
    "mime-types": "^2.1.25",
    "progress": "^2.0.3",
    "proxy-from-env": "^1.0.0",
    "rimraf": "^3.0.0",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/extract-zip": "^1.6.2",
    "@types/mime": "^2.0.1",
    "@types/mime-types": "^2.1.0",
    "@types/node": "8",
    "@types/proxy-from-env": "^1.0.0",
    "@types/rimraf": "^2.0.3",
    "@types/ws": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "commonmark": "^0.29.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.2",
    "jpeg-js": "^0.3.6",
    "minimist": "^1.2.0",
    "ncp": "^2.0.0",
    "pixelmatch": "^5.1.0",
    "pngjs": "^3.4.0",
    "text-diff": "^1.0.1",
    "ts-node": "^8.5.4",
    "typescript": "~3.7.3"
  },
  "browser": {
    "./lib/BrowserFetcher.js": false,
    "ws": "./utils/browser/WebSocket",
    "fs": false,
    "child_process": false,
    "rimraf": false,
    "readline": false
  }
}
